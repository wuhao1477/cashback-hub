{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.backend.tsbuildinfo",
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ES2020",
    "lib": ["ES2022"],
    "types": ["node"],
    "resolveJsonModule": true
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts"],
  "exclude": ["dist", "node_modules"]
}
